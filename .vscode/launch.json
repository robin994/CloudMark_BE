  "version": "0.2.0",
  "configurations": [
    {
      "name": "Create CloudMark Schema",
      "type": "python",
      "request": "launch",
      "module": "CreateCloudMark",
      "justMyCode": true,
      "env": {
        "PYTHONPATH": "${workspaceFolder}/Dao;${workspaceFolder};${workspaceFolder}/DB"
      }
    },
    {
      "name": "Insert records into CloudMark Schema",
      "type": "python",
      "request": "launch",
      "module": "InsertCloudMark",
      "justMyCode": true,
      "env": {
        "PYTHONPATH": "${workspaceFolder}/Dao;${workspaceFolder};${workspaceFolder}/DB"
      }
    },
    {
      "name": "Python: FastAPI",
      "type": "python",
      "request": "launch",
      "console": "integratedTerminal",
      "module": "uvicorn",
      "args": [
        "slack:app",
        "--reload"
      ],
      "env": {
        "PYTHONPATH": "${workspaceFolder}/"
      },
      "jinja": true,
      "justMyCode": true
    },
    {
      "name": "Test Dao",
      "type": "python",
      "request": "launch",
      "module": "test",
      "justMyCode": true,
      "env": {
        "PYTHONPATH": "${workspaceFolder}/Dao;${workspaceFolder};${workspaceFolder}/DB"
      }
    },
    {
      "name": "Docker: Python - General",
      "type": "docker",
      "request": "launch",
      "preLaunchTask": "docker-run: debug",
      "python": {
        "pathMappings": [
          {
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "/app"
          }
        ],
        "projectType": "general"
      }
    }
  ]
}